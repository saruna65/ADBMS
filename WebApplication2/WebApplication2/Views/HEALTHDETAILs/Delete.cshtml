@model WebApplication2.Models.HEALTHDETAIL

@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>GYM Management System</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <style>
        .h1style {
            margin-top: 100px;
            text-align: center;
            letter-spacing: 0.2cm;
        }

        .formmargin {
            margin-top: 30px;
        }

        hr.solid {
            border-top: 3px solid #bbb;
        }

        .h5margin {
            margin-top: 30px;
        }
    </style>
</head>
<body>
    <nav class="navbar fixed-top navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">GYM</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            MEMBERS
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("MEMBERS", "Index", "MEMBERs", new { area = "" }, new { @class = "dropdown-item" })</li>
                            <li>@Html.ActionLink("MEMBER CREDENTIALS", "Index", "MEMBERCREDs", new { area = "" }, new { @class = "dropdown-item" })</li>
                            <li>@Html.ActionLink("EXPIRED MEMBERS", "Index", "EXPIREDMEMBERs", new { area = "" }, new { @class = "dropdown-item" })</li>
                            <li><hr class="dropdown-divider"></li>
                            <li>@Html.ActionLink("HEALTH DETAILS", "Index", "HEALTHDETAILs", new { area = "" }, new { @class = "dropdown-item" })</li>
                        </ul>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            MEMBERSHIPS
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("MEMBERSHIPS", "Index", "MEMBERSHIPs", new { area = "" }, new { @class = "dropdown-item" })</li>
                            <li>@Html.ActionLink("EXPIRED MEMBERS", "Index", "EXPIREDMEMBERs", new { area = "" }, new { @class = "dropdown-item" })</li>
                            <li><hr class="dropdown-divider"></li>
                            

                        </ul>
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("BOOKINGS", "Index", "BOOKINGs", new { area = "" }, new { @class = "nav-link" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("ATTENDANCE", "Index", "ATTENDANCEs", new { area = "" }, new { @class = "nav-link" })
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <h1 class="h1style">HEALTH DETAILS</h1>
        <div class="container text-center navmargin">
            <div class="row align-items-center">
                <div class="col">
                </div>
                <div class="col">
                </div>
                <div class="col">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <input type="submit" class="btn btn-danger" value="DELETE" />
                    }
                </div>
            </div>
        </div>
    </div>
    <div class="container formmargin">
        <div class="card">
            <div class="card-body">
                <div class="row align-items-start">
                    <h5>Details</h5>
                    <hr class="solid">
                    <div class="col">
                        <table class="table table-borderless">
                            <thead>
                            <th>Membership No</th>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="table-active">
                                        @if (Model.MEMBERSHIP_NO != null)
                                        {
                                            @Model.MEMBERSHIP_NO.ToString()
                                        }
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-borderless">
                            <thead>
                            <th>Disabilities</th>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="table-active">@Html.DisplayFor(model => model.DISABILITIES)</td>
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-borderless">
                            <thead>
                            <th>Height</th>
                            </thead>
                            <tbody>
                                <tr>
                                    <tr>
                                    <td class="table-active">@Html.DisplayFor(model => model.HEIGHT)</td>

                                   
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="col">

                        <table class="table table-borderless">
                            <thead>
                            <th>Weight</th>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="table-active">@Html.DisplayFor(model => model.WEIGHT)</td>
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-borderless">
                            <thead>
                            <th>BMI INDEX</th>
                            </thead>
                            <tbody>
                                <tr>                              
                                    <td class="table-active">@Html.DisplayFor(model => model.BMI)</td>                                    
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-borderless">
                            <thead>
                            <th>BMI Category</th>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="table-active">@Html.DisplayFor(model => model.BMI_CAT)</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
